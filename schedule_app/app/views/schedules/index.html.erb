<p id="notice"><%= notice %></p>

<%= link_to "Return home after uploading", root_path %>

<h1>Schedules</h1>

<table>
  <thead>
    <tr>
      <th>Building</th>
      <th>Number</th>
      <th>Date</th>
      <th>Time</th>
      <th>Avail</th>
      <th>Type</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @schedules.each do |schedule| %>
      <tr>
        <td><%= schedule.building %></td>
        <td><%= schedule.number %></td>
        <td><%= schedule.date %></td>
        <td><%= schedule.time %></td>
        <td><%= schedule.avail %></td>
        <td><%= schedule.booking_type %></td>
        <td><%= link_to 'Show', schedule %></td>
        <td><%= link_to 'Edit', edit_schedule_path(schedule) %></td>
        <td><%= link_to 'Destroy', schedule, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Schedule', new_schedule_path %>
